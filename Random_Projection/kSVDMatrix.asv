function [A] = kSVDMatrix(m, n, k, r)
% This is the k-SVD matrix generation algorithm
% ======================================
% Input: A: matrix to generate, A is m by n
%        k: first k singular value is dominant  
%        r: rank of A
% ======================================

s = zeros(r,1);
for i = 1:k
   s(i) = 10*(2*k+1-i)/k + rand; 
end

for i = k+1:r
   s(i) = 5*(2*k+1-i)/k + rand; 
end

U = get_othonormal()

end

